{
  "name": "bug-story",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/audit-logging": "^5.1.0",
    "@sap/cds": "^5",
    "@sap/cds-odata-v2-adapter-proxy": "^1.7.15",
    "@sap/hana-client": "^2.10.15",
    "@sap/xsenv": "^3.1.1",
    "@sap/xssec": "^3.2.10",
    "dayjs": "^1.10.7",
    "express": "^4",
    "helmet": "^4.6.0",
    "passport": "^0.5.0",
    "passport-oauth2": "^1.6.1"
  },
  "scripts": {
    "startdev": "npx cds run",
    "start": "cds run",
    "test": "mocha test/project_tests.js --timeout 15000 --exit"
  },
  "cds": {
    "requires": {
      "uaa": {
        "kind": "xsuaa"
      },
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": "db/bugstory.db"
        }
      },
      "userExtService": {
        "kind": "rest",
        "credentials": {
          "destination": "userBugDestinationSrv",
          "path": "/Users",
          "requestTimeout": 30000
        },
        "pool": {
          "min": 1,
          "max": 10
        }
      }
    },
    "odata": {
      "version": "v4"
    }
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.90.13",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "mocha": "^9.1.3",
    "sqlite3": "^5.0.2"
  },
  "sapux": [
    "app/project1",
    "app/usermanagement"
  ]
}
